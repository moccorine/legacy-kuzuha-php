{
  "name": "moccorine/legacy-kuzuha-php",
  "description": "Legacy Kuzuha PHP BBS",
  "type": "project",
  "license": "proprietary",
  "autoload": {
    "psr-4": {
      "App\\": "src/",
      "Kuzuha\\": "src/Kuzuha/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "require": {
    "slim/slim": "^4.15",
    "slim/psr7": "^1.8",
    "vlucas/phpdotenv": "^5.6",
    "twig/twig": "^3.22",
    "symfony/translation": "^7.3",
    "php-di/php-di": "^7.1"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.89",
    "rector/rector": "^2.2",
    "pestphp/pest": "^4.1",
    "phpunit/phpunit": "^12.4"
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
